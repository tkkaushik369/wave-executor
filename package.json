{
	"name": "socketcontrol",
	"productName": "socketcontrol",
	"version": "1.0.0",
	"description": "Multiplayer Sketchbook",
	"main": ".webpack/main",
	"scripts": {
		"setup": "npm install && npm fund && npm run build",
		"build": "webpack --config ./src/webpack.prod.js && tsc --project ./src/server/tsconfig.server.json",
		"dev": "concurrently -k \"tsc -p ./src/server -w\" \"nodemon ./dist/server/server.js\" \"webpack serve --config ./src/webpack.dev.js\"",
		"start-old:server": "node ./dist/server/server.js",
		"fcm": "node forgeConfigMaker.js",
		"start": "npm run fcm && electron-forge start",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"lint": "eslint --ext .ts,.tsx .",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
